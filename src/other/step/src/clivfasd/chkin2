
while [ "$1" ]
do
  co -l $1
  mv $1 $1.30
  echo "co -r2.2 -l $1"
  co -r2.2 -l $1
  mv $1.30 $1
  echo " "
  echo "checking in $1"
  echo "check in v 3.0"
  echo " "
  ci -f -r3.0 -sdp3-0 -m"STEP Class Library Pre Release 3.0" $1
  rcs -u $1
  co -l -r3.0 $1
  echo " "
  echo "check in v 3.0.1.0"
  echo " "
  ci -f -r3.0.1.0 -m"Setting the first branch" $1
  rcs -u $1
  rcs -b3.0.1 $1
  co $1
  rm $1.30
  shift
done
